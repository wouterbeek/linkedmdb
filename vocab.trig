  vocab:FilmGenre
    rdfs:label "film genre"@en;
    rdfs:subClassOf owl:Thing.

  vocab:FilmJob
    rdfs:label "film job"@en;
    rdfs:subClassOf owl:Thing.

  vocab:FilmLocation
    rdfs:label "film location"@en;
    rdfs:subClassOf sdo:Place.

  vocab:FilmSeries
    rdfs:label "film series"@en;
    rdfs:subClassOf owl:Thing.

  vocab:FilmSetDesigner
    rdfs:label "film set designer"@en;
    rdfs:subClassOf sdo:Person.

  vocab:FilmSubject
    rdfs:label "film subject"@en;
