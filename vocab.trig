  vocab:Producer
    rdfs:label "producer"@en;
    rdfs:subClassOf sdo:Person.

  vocab:ProductionCompany
    rdfs:label "production company"@en;
    rdfs:subClassOf sdo:Corporation.

  vocab:Writer
    rdfs:label "writer"@en;
    rdfs:subClassOf sdo:Person.
}
